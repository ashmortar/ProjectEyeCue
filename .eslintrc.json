{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "no-console": "off",
    "linebreak-style": "off",
    "global-require": [0],
    "no-plusplus": "off",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "prefer-const": "off",
    "no-case-declarations": "off",
    "eqeqeq": "off",
    "object-curly-newline": "off",
    "react/jsx-boolean-value": "off",
    "no-nested-ternary": "off",
    "quotes": "off",
    "no-else-return": "off",
    "react/sort-comp": "off"
  }
}
